{
  "name": "@tansuk/rott-ui",
  "version": "0.1.0",
  "description": "This is property based full built-in UI Kit for React-Native",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace @tansuk/rott-ui-example",
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "del-cli lib",
    "prepare": "bob build",
    "release": "release-it --only-version",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json,md}\"",
    "lint:fix": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "format:lint": "yarn format && yarn lint:fix"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tans-uk/rott-ui.git"
  },
  "author": "DoÄŸukan Tansuk <dogukantansuk@gmail.com> (https://github.com/Tans-uk/rott-ui)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tans-uk/rott-ui/issues"
  },
  "homepage": "https://github.com/Tans-uk/rott-ui#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@evilmartians/lefthook": "^1.5.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@react-native/babel-preset": "0.78.2",
    "@react-native/eslint-config": "^0.78.0",
    "@release-it/conventional-changelog": "^9.0.2",
    "@shopify/flash-list": "1.8.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "13.2.0",
    "@types/jest": "^29.5.5",
    "@types/react": "^19.0.12",
    "commitlint": "^19.6.1",
    "del-cli": "^5.1.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "19.0.0",
    "react-intl": "^7.1.11",
    "react-native": "0.79.4",
    "react-native-builder-bob": "^0.40.8",
    "react-native-date-picker": "^5.0.13",
    "react-native-keyboard-controller": "1.17.3",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mask-input": "^1.2.3",
    "react-native-reanimated": "^3.18.0",
    "react-native-safe-area-context": "5.4.1",
    "react-native-select-contact": "^1.6.3",
    "react-native-svg": "^15.12.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-tab-view": "4.0.2",
    "react-native-toast-notifications": "^3.4.0",
    "release-it": "^17.10.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@shopify/flash-list": ">=1.8.0",
    "react": "*",
    "react-intl": ">=7.1.0",
    "react-native": "*",
    "react-native-date-picker": ">=5.0.0",
    "react-native-keyboard-controller": ">=1.17.3",
    "react-native-linear-gradient": "2.8.3",
    "react-native-mask-input": ">=1.2.3",
    "react-native-reanimated": ">=3.0.0",
    "react-native-safe-area-context": ">=5.4.1",
    "react-native-select-contact": ">=1.6.3",
    "react-native-svg": ">=15.12.0",
    "react-native-svg-transformer": ">=1.5.1",
    "react-native-tab-view": ">=4.0.2",
    "react-native-toast-notifications": ">=3.4.0"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@3.6.1",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "angular"
        }
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "create-react-native-library": {
    "languages": "js",
    "type": "library",
    "version": "0.51.1"
  }
}
